        "id": {
          "type": "string",
          "title": "generated uuid in database"
        },
        "name": {
          "type": "string",
          "title": "used to partition Attribute Definitions, support by namespace AuthN and enable federation"
        },
        "fqn": {
          "type": "string"
        },
        "active": {
          "type": "boolean",
          "title": "active by default until explicitly deactivated"
